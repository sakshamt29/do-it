{
  "manifest_version": 2,
  "name": "Do It! A to-do list app",
  "version": "1.0",
  "description": "A simple and elegant to-do list extension ",
  "icons": {
    "128": "./icons/128.png",
    "16": "./icons/16.png"
  },
  "browser_action": {
    "default_icon": "./icons/16.png",
    "default_popup": "popup.html"
  },
  "permissions": ["storage"]
}
